{
  "name" : "wrapper_bootstrap_fontawesome",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/wrapper_bootstrap_fontawesome",
  "dependencies": {
  },
  "peerDependencies": {
  },
  "caleydo": {
    "dependencies": {
      "web": {
        "font-awesome": ">=4.3.0",
        "bootstrap": "~3.3.4",
        "text": "~2.0.14"
      }
    },

    "requirejs-config": {
      "paths": {
        "bootstrap": "${baseUrl}/bootstrap/dist/js/bootstrap"
      },
      "map": {
        "*": {
         "font-awesome": "${baseUrl}/require-css/css.js!${baseUrl}/font-awesome/css/font-awesome.min"
        }
      },
      "shim": {
        "bootstrap": [
          "jquery",
          "css!${baseUrl}/bootstrap/dist/css/bootstrap"
        ]
      }
    }
  }
}
