{
  "name": "caleydo_bootstrap_fontawesome",
  "description": "Wrapper for integration of Bootstrap and FontAwesome along with a common header for Caleydo Web apps",
  "version": "0.0.1",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/caleydo_bootstrap_fontawesome",
  "dependencies": {
  },
  "peerDependencies": {
  },
  "caleydo": {
    "type": "plugin",
    "category": "utility",
    "plugins": {
      "web": [
        {
          "type": "ajax-adapter",
          "id": "caleydo_adapter_jquery",
          "file": "ajax_adapter_jquery"
        }
      ]
    },
    "dependencies": {
      "web": {
        "font-awesome": ">=4.3.0",
        "bootstrap": "~3.3.4",
        "text": "~2.0.14"
      },
      "tsd": {
        "jquery": "dca2d4",
        "bootstrap": "1f0791"
      }
    },
    "requirejs-config": {
      "paths": {
        "bootstrap": "${baseUrl}/bootstrap/dist/js/bootstrap"
      },
      "map": {
        "*": {
          "font-awesome": "${baseUrl}/require-css/css.js!${baseUrl}/font-awesome/css/font-awesome.min"
        }
      },
      "shim": {
        "bootstrap": [
          "jquery",
          "css!${baseUrl}/bootstrap/dist/css/bootstrap"
        ]
      }
    }
  }
}
